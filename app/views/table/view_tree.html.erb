<head>
  <style>
      html {
          position: relative;
          min-height: 100%;
      }

      body {
          margin-bottom: 40px;
      }

      main {
          margin-top: 15px;
      }

      footer {
          height: 40px;
          position: absolute;
          bottom: 0;
          width: 100%;
      }

      .my-primary {
          background-color: #fb607f;
          color: white;
      }

      #wrapper {
          max-width: 960px;
          margin: auto;
      }


      img{
          max-width: 56px
      }

      form{
          width: 50%;
          font-family:  "Lohit Tamil Classical";
          color: #fb607f;
          float: left;
          padding: 10px;
          border-radius: 10px;
         border: 1px ;
      }
      .all {
          margin-top: 15px;
          position: relative;
          color: #fb607f;
          border: 1px solid #fb607f;
          font-family:  "Lohit Tamil Classical";
      }


  </style>
</head>
<body>
<div id="wrapper">
  <nav class="navbar navbar-expand-lg navbar-dark my-primary">
    <p href="/"><img src="data:image/png;base64," alt="Logo" id="img"><%= link_to t("item.logout"), session_logout_path %></p>
  </nav>
  <div class="heading">
    <%= form_tag("/table/view_one", :method => "get") do %>
      <%= label_tag(t("item.input_name")) %>
      <br/>
      <%= text_field_tag(:name) %>
      <br/>
      <%= label_tag(t("item.input_profession")) %>
      <br/>
      <%= text_field_tag(:prof) %>
      <br/>
      <%= label_tag(t("item.input_day")) %>
      <br/>
      <%= text_field_tag(:day) %>
      <br/>
      <%= label_tag(t("item.input_time")) %>
      <br/>
      <%= text_field_tag(:time) %>
      <br/>
      <%= label_tag(t("item.input_cabinet")) %>
      <br/>
      <%= text_field_tag(:cab) %>
      <br/>
      <%= submit_tag(t("item.entr_info"),:class=>"all") %>
      <%= hidden_field_tag(:nik,"#{@nik}") %>
      <%= hidden_field_tag(:locale,"#{@locale}") %>
    <% end %>
    <%= form_tag("/table/cached", :method => "get") do %>
      <%= submit_tag(t("item.tab_db"),:class=>"all") %>
    <% end %>
    <%= form_tag("/table/cached_two", :method => "get") do %>
      <%= submit_tag(t("item.app_db"),:class=>"all") %>
    <% end %>
    <%= form_tag("/table/input", :method => "get") do %>
      <%= hidden_field_tag(:nik,"#{@nik}") %>
      <%= hidden_field_tag(:locale,"#{@locale}") %>
      <%= submit_tag(value =t("item.back"),:class=>"all") %>
    <% end %>
  </div>
  <aside>
    <%= link_to t("item.show_all_users"), users_path(:nik=> params[:nik]) %>
  </aside>
</div>
<footer class="my-primary">
  <div class="container">
    <p class="footer-text"><%=t("item.polyclinic") %></p>
  </div>
</footer>
</body>